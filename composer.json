{
  "name": "bigpixelrocket/laravel-omakase",
  "description": "A Laravel package that provides the omakase command",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Bigpixelrocket\\LaravelOmakase\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Lucian Văcăroiu",
      "email": "lucian@bigpixelrocket.com"
    }
  ],
  "require": {
    "php": "^8.4",
    "laravel/framework": "^12.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Bigpixelrocket\\LaravelOmakase\\OmakaseServiceProvider"
      ]
    }
  },
  "require-dev": {
    "laravel/pint": "^1.22",
    "larastan/larastan": "^3.4",
    "barryvdh/laravel-ide-helper": "^3.5",
    "pestphp/pest": "^3.8",
    "orchestra/testbench": "^10.0",
    "pestphp/pest-plugin-laravel": "^3.2"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  }
}
